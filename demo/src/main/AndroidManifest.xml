        <activity
            android:name="de.quist.app.mapsabstraction.example.TileOverlayDemoActivity"
            android:label="@string/tile_overlay_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.UiSettingsDemoActivity"
            android:label="@string/ui_settings_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.VisibleRegionDemoActivity"
            android:label="@string/visible_region_demo_label"/>
