        android:label="@string/demo_title"
        android:hardwareAccelerated="true">

        <activity android:name="de.quist.app.mapsabstraction.example.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="de.quist.app.mapsabstraction.example.BasicMapDemoActivity"
            android:label="@string/basic_map_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.CameraDemoActivity"
            android:label="@string/camera_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.CircleDemoActivity"
            android:label="@string/circle_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.EventsDemoActivity"
            android:label="@string/events_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.GroundOverlayDemoActivity"
            android:label="@string/ground_overlay_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.IndoorDemoActivity"
            android:label="@string/indoor_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.LayersDemoActivity"
            android:label="@string/layers_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.LocationSourceDemoActivity"
            android:label="@string/location_source_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.MapInPagerDemoActivity"
            android:label="@string/map_in_pager_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.MarkerDemoActivity"
            android:label="@string/marker_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.MultiMapDemoActivity"
            android:label="@string/multi_map_demo_label"/>
        <activity
            android:name="de.quist.app.mapsabstraction.example.OptionsDemoActivity"
